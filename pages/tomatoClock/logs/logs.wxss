.container {
    padding-bottom: 50px;
    max-height: 92%;
    position: relative;
}

.log {
    color: #999;
    margin: 15px 10px;
}

.log_item {
    padding: 15px 5px;
}

.clear {
    position: absolute;
    bottom: 100rpx;
    width: 100%;
    text-align: center;
}

.log_action {
    float: right;
}

.clear .clear_btn {
    margin: 10px;
    height: 35px;
    line-height: 35px;
    color: #fff;
    background-color: #fd8f58;
    border-radius: 2px;
    width: 80%;
}