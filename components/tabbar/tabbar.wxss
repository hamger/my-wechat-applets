.container {
    min-height: 100%;
    padding-bottom: 100rpx;
}

.tab-bar {
    width: 100%;
    height: 110rpx;
    position: fixed;
    bottom: 0;
    display: flex;
    z-index: 999;
    background-color: #fff;
    border-top: 1px solid rgb(213, 213, 213);
}

.section_item {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}

.section_item .section_image {
    width: 65rpx;
    height: 65rpx;
}

.section_item .section_title {
    font-size: 20rpx;
    font-weight: 300;
}


